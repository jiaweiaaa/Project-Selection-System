tle:'',\r\n        details:'',\r\n        skills:'',\r\n        labelPosition: 'top',\r\n        Project:{\r\n            id:'',\r\n            client:'',\r\n            title:'',\r\n            details:'',\r\n            skills:'',\r\n        },\r\n      };\r\n    },\r\n    created(){\r\n        this.getProjectList()\r\n        console.log(this.currentStudent.groupId)\r\n        this.groupId = this.currentStudent.groupId\r\n    },\r\n    methods:{\r\n        getProject(id ,client,title,details,skills){\r\n            this.id = id\r\n            this.client = client\r\n            this.title = title\r\n            this.details = details\r\n            this.skills =skills\r\n        },\r\n        getProjectList(){\r\n            Project.getAllProject()\r\n            .then((response) => {\r\n                console.log( response.data.Project)\r\n                this.Project = response.data.Project\r\n            })\r\n        },\r\n        preferChoose(pre1,pre2,pre3,groupId){\r\n            Project.allocated(pre1,pre2,pre3,groupId)\r\n            .then((response) =>{\r\n                this.pre1 = pre1\r\n                this.pre2 = pre2\r\n                this.pre3 = pre3\r\n                this.groupId = this.currentStudent.groupId\r\n                alert(\"Select Successfully!\")\r\n            })\r\n        },\r\n        setCenter(index){\r\n            this.selectIndex = index\r\n        }\r\n  },\r\n   computed: {\r\n        ...mapGetters([\r\n            'currentStudent'\r\n        ]),\r\n   }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" >\r\n.project-container{\r\n    font-size: 20px;\r\n    font-family: \"sans-serif\";\r\n    .background-container {\r\n        padding: 5px;\r\n        box-sizing: border-box;\r\n        height:calc(90vh - 70px)\r\n    }\r\n    .selectedColor{\r\n        border-color: #FFD633;\r\n    }\r\n    .box-card{\r\n        display: block;\r\n        height:185px;\r\n        margin-bottom: 20px;\r\n        .title {\r\n            margin-bottom: 15px;\r\n        }\r\n        .el-form-item {\r\n            margin-bottom: 0px;\r\n        }\r\n        .el-form--inline .el-form-item__label {\r\n            float: none;\r\n            display: inline-block;\r\n            font-size: 17px;\r\n            font-family: \"sans-serif\";\r\n            margin-bottom: -10px;\r\n        }\r\n        .el-input--suffix .el-input__inner {\r\n            padding-right: 30px;\r\n            border-width: revert;\r\n            border-color: #aaa9a9;\r\n        }\r\n        .el-input__inner:focus{\r\n            border-color:#FFD633\r\n        }\r\n        .preference{\r\n            display: block;\r\n            border-radius: 4px;\r\n            border-width: 10px;\r\n           .pre1{\r\n               margin-right: 3%;\r\n           }\r\n           .pre2{\r\n               margin-right: 3%;\r\n           }\r\n           .submit{\r\n               margin-top:4%;\r\n               margin-left:6%;\r\n               padding:0px;\r\n           }\r\n        }\r\n        .el-button--submit {\r\n        background: #FFD633;\r\n        border-color: #FFD633;\r\n        color: #fff;\r\n        width:130px;\r\n        height:40px;\r\n        font-size: 18px;\r\n        }\r\n    }\r\n    .el-form-item__label {\r\n        text-align: right;\r\n        vertical-align: middle;\r\n        float: left;\r\n        color: #303133;\r\n        line-height: 40px;\r\n        box-sizing: border-box;\r\n        font-size: 17px;\r\n        font-weight: bold;\r\n    }\r\n    .el-input.is-disabled .el-input__inner {\r\n        background-color: #ffffff;\r\n        border-color: #ffffff;\r\n        color: #303133;\r\n        cursor: not-allowed;\r\n    }\r\n    .el-textarea.is-disabled .el-textarea__inner {\r\n        background-color: #ffffff;\r\n        border-color: #ffffff;\r\n        color: #303133;\r\n        cursor: not-allowed;\r\n    }\r\n    .main-box{\r\n        .left-card{\r\n            height:calc(90vh - 50px);\r\n         