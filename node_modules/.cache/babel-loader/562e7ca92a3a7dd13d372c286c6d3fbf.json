>\r\n                    <p class=\"name\">{{student.name}}</p>\r\n                    <p class=\"email\">{{student.email}}</p>\r\n                    \r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport announcementApi from '@/api/announcement'\r\nimport student from '@/api/student'\r\nimport { mapGetters } from 'vuex'\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            annList: [],\r\n            menberList:[],\r\n            groupId:'',\r\n            studentId:''\r\n        }\r\n    },\r\n    created() {\r\n        \r\n        this.getAnnouncementList()\r\n        console.log(this.currentStudent.groupId)\r\n        this.groupId = this.currentStudent.groupId\r\n        this.getGroupMember(this.groupId)\r\n        this.studentId=this.currentStudent.id\r\n    },\r\n    computed: {\r\n        ...mapGetters([\r\n            'currentStudent'\r\n        ]),\r\n   },\r\n    methods: {\r\n        getAnnouncementList() {\r\n            announcementApi.getAnnouncementList().then( response => {\r\n                console.log(response)\r\n                this.annList = response.data.list\r\n            })\r\n        },\r\n        replaceImage (content) {\r\n            content = content.replace(/<img.*?>/g, ' [image] ')\r\n            return content;\r\n        },\r\n        getGroupMember(groupId) {\r\n            announcementApi.getGroupMember(groupId).then( response => {\r\n                console.log(response)\r\n                this.groupId = this.currentStudent.groupId\r\n                this.menberList = response.data.groupMember\r\n            })\r\n        },\r\n        exitGroup(id) {\r\n            student.exitGroup(id).then( response => {\r\n                console.log(response)\r\n                this.currentStudent.groupId=''\r\n                this.groupId = ''\r\n                this.getGroupMember(this.groupId)\r\n            })\r\n        },\r\n\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n.home-page {\r\n    width:1301px;\r\n    height:657px;\r\n    background:#f8f8f8 url('../../assets/homeCircle.png') no-repeat 308px bottom;\r\n}\r\n.left{\r\n    width:778px;\r\n    height:657px;\r\n    float: left;\r\n    .announcementTitle{\r\n        width:737px;\r\n        height:20px;\r\n        padding: 110px 0 0 0;\r\n        font-weight: bold;\r\n        background: url('../../assets/annLogo.png') no-repeat;\r\n        font-size: 18px;\r\n    }\r\n    .annos{\r\n        width:675px;\r\n        height:452px;\r\n        padding: 82px 0 0 62px;\r\n    }\r\n    .background-bar{\r\n        height:261px;\r\n    }\r\n    .ann{\r\n        width:675px;\r\n        height:99px;\r\n        font-weight: bold;\r\n        font-size: 18px;\r\n        padding-bottom: 30px;\r\n        position: relative;\r\n        display: flex;\r\n        .content{\r\n            word-break: break-all;\r\n            white-space: break-spaces;\r\n            display: -webkit-box;\r\n            -webkit-box-orient: vertical;\r\n            -webkit-line-clamp: 5;\r\n            overflow: hidden;\r\n        }\r\n    }\r\n    \r\n}\r\n.right{\r\n    float: left;\r\n    .link{\r\n        width:523px;\r\n        height:218px;\r\n        border-radius: 6px;\r\n        background: #ffffff;\r\n        .img_group{\r\n            margin-left: 17px;\r\n        }\r\n    }\r\n    .groupInfo{\r\n        width:523px;\r\n        height:353px;\r\n        border-radius: 6px;\r\n        background: #ffffff;\r\n        margin-top: 50px;\r\n        .gTop{\r\n            width:491px;\r\n            height:45px;\r\n            padding: 22px 0 0 32px;\r\n            .groupTitle{\r\n                width:137px;\r\n                margin-right: 291px;\r\n                float: left;\r\n            }\r\n            i{\r\n                width:45px;\r\n                height:30px;\r\n                font-size: 30px;\r\n                color: #f9ec95;\r\n                float: left;\r\n                text-align: 30px;\r\n            }\r\n        }\r\n        .gId{\r\n            width:491px;\r\n            height:24px;\r\n            font-size: 18px;\r\n            font-weight: bold;\r\n            padding-left: 32px;\r\n        } \r\n        .menberList{\r\n            padding-left: 32px;\r\n            .menber{\r\n                height:45px;\r\n                width:470px;\r\n                border-bottom: #eeeeee 2px solid;\r\n                .name{\r\n                    width:130px;\r\n                    padding-left: 5px;\r\n                    \r\n                    float: left;\r\n                }\r\n                .email{\r\n                    width:304px;\r\n                   \r\n                    \r\n                    float: left;\r\n                }\r\n               \r\n            }\r\n\r\n        }\r\n    }\r\n}\r\n</style>"],"sourceRoot":"src/views/home"}]}